	"files": [
		"index.js",
		"index.d.ts"
	],
	"keywords": [
		"fetch",
		"whatwg",
		"api",
		"request",
		"response",
		"http",
		"client",
		"httperror",
		"utilities",
		"wrapper",
		"ky",
		"got",
		"axios"
	],
	"devDependencies": {
		"ava": "^6.2.0",
		"typescript": "^5.7.3",
		"xo": "^0.60.0"
	},
	"xo": {
		"rules": {
			"n/no-unsupported-features/node-builtins": "off"
		}
	}
}
