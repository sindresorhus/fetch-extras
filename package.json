{
	"name": "fetch-extras",
	"version": "1.0.0",
	"description": "Useful utilities for working with Fetch",
	"license": "MIT",
	"repository": "sindresorhus/fetch-extras",
	"funding": "https://github.com/sponsors/sindresorhus",
	"author": {
		"name": "Sindre Sorhus",
		"email": "sindresorhus@gmail.com",
		"url": "https://sindresorhus.com"
	},
	"type": "module",
	"exports": {
		"types": "./index.d.ts",
		"default": "./index.js"
	},
	"sideEffects": false,
	"engines": {
		"node": ">=18.18"
	},
	"scripts": {
		"test": "xo && ava && tsc index.d.ts"
